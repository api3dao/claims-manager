{
  "name": "@api3/claims-manager",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && yarn build:contracts && yarn build:contract-types && yarn build:tsc",
    "build:contracts": "hardhat compile",
    "build:contract-types": "yarn typechain --target ethers-v5 --out-dir ./src/contracts \"./artifacts/contracts/**/!(*.dbg).json\"",
    "build:tsc": "tsc -p tsconfig.build.json",
    "clean": "rimraf -rf ./dist *.tgz src/contracts artifacts cache",
    "lint": "yarn prettier:check && yarn lint:eslint && yarn lint:solhint",
    "lint:eslint": "eslint . --ext .js,.ts",
    "lint:solhint": "solhint contracts/**/*.sol",
    "prepare": "husky install",
    "prettier": "yarn prettier:write",
    "prettier:write": "prettier --write \"./**/*.{json,js,ts,sol,md}\"",
    "prettier:check": "prettier --check \"./**/*.{json,js,ts,sol,md}\"",
    "test": "hardhat test --parallel",
    "test:coverage": "hardhat coverage",
    "test:gas": "REPORT_GAS=true hardhat test"
  },
  "engines": {
    "node": "^18.16.0"
  },
  "main": "dist/src/index",
  "files": [
    "contracts",
    "dist"
  ],
  "devDependencies": {
    "@api3/airnode-protocol-v1": "^0.5.2",
    "@api3/api3-dao-contracts": "^0.1.0",
    "@kleros/erc-792": "^8.0.0",
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@typechain/ethers-v5": "^10.2.1",
    "chai": "^4.3.6",
    "eslint": "^8.14.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.5",
    "hardhat": "^2.11.1",
    "hardhat-gas-reporter": "^1.0.8",
    "husky": "^7.0.4",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.7",
    "solidity-coverage": "^0.8.2",
    "typechain": "^8.1.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@openzeppelin/contracts": "4.5.0"
  }
}
